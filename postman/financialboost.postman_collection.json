        {
          "name": "Login - sucesso (gera token)",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test(\"Status 200 para login\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "const json = pm.response.json();",
                  "pm.test(\"Token presente na resposta\", function () {",
                  "    pm.expect(json.token).to.be.a('string').and.not.empty;",
                  "});",
                  "pm.collectionVariables.set(\"authToken\", json.token);"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"login\": \"{{userLogin}}\",\n    \"password\": \"{{userPassword}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "auth",
                "login"
              ]
            },
            "description": "Autentica e persiste o JWT em {{authToken}} para uso nos demais requests protegidos."
          }
        },
